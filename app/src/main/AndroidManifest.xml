<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gowo">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.AjudaActivity"
            android:parentActivityName=".activity.PerfilActivity" />
        <activity
            android:name=".activity.ConfiguracoesActivity"
            android:parentActivityName=".activity.PerfilActivity" />
        <activity
            android:name=".activity.InfoPessoaisActivity"
            android:parentActivityName=".activity.PerfilActivity" />
        <activity
            android:name=".activity.FeedActivity"
            android:parentActivityName=".activity.HomeActivity" />
        <activity
            android:name=".activity.ChatPrivActivity"
            android:parentActivityName=".activity.ChatFeedActivity" />
        <activity
            android:name=".activity.MeusServicosActivity"
            android:parentActivityName=".activity.PerfilActivity" />
        <activity
            android:name=".activity.CadastrarUsuarioActivity"
            android:parentActivityName=".activity.MainActivity" />
        <activity
            android:name=".activity.CadastrarServicoActivity"
            android:parentActivityName=".activity.MeusServicosActivity" />
        <activity
            android:name=".activity.ServicoActivity"
            android:parentActivityName=".activity.FeedActivity" />
        <activity android:name=".activity.PerfilActivity" />
        <activity android:name=".activity.ChatFeedActivity" />
        <activity android:name=".activity.HomeActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>